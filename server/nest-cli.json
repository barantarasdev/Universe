{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": "public/images/",
        "outDir": "dist/public/images",
        "watchAssets": true
      },
      {
        "include": "public/musics/",
        "outDir": "dist/public/musics",
        "watchAssets": true
      }
    ],
    "watchAssets": true,
    "deleteOutDir": true
  }
}
